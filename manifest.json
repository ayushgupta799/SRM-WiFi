{
  "manifest_version": 2,
  "name": "SRM wifi",
  "version": "0.1",
  "permissions":[
    "activeTab"
  ],
  "background": {
      "scripts": ["background.js"]
    },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
      "default_icon": "logo.png"
    }
}